<!-- books.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title>Music Name</title>
  <link rel="stylesheet" href="../css/style.css">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

</head>
<body>
  <h1>Music Name</h1>
  <table class="book-table">
    <tr>
      <th>id_mus</th>
      <th>Music_Name</th>
      <th>Actions</th>
      
    </tr>
    <% for(var i=0; i < music.length; i++) { %>
    <tr>
      <td><%= music[i].id_mus %></td>
      <td><%= music[i].music_name %></td>
      
      <td>
        <a href="/updatems/<%= music[i].id_mus %>">Edit</a>
        <a href="/delete2/<%= music[i].id_mus %>">Delete</a>
      </td>
    </tr>
    <% } %>
  </table>
  <div class="link-center">
    <a href="/" class="btn btn-success">Back to MusicAlbum</a>
    <a href="/inab3" class="btn btn-danger">Create New Music</a>
  </div>
</body>
</html>
